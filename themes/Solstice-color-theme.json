      }
    },
    {
      "name": "Haskell - Type Variables",
      "scope": ["variable.other.generic-type.haskell"],
      "settings": {
        "foreground": "#fb8c67"
      }
    },
    {
      "name": "Haskell - Pattern Matching",
      "scope": ["keyword.other.arrow.haskell"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Haskell - Where/Let/In",
      "scope": ["keyword.other.haskell"],
      "settings": {
        "foreground": "#fa409d"
      }
    },
    {
      "name": "Haskell - Qualified Imports",
      "scope": ["entity.name.function.haskell"],
      "settings": {
        "foreground": "#bf95ff"
      }
    },
    {
      "name": "Haskell - Strings",
      "scope": ["string.quoted.double.haskell", "string.quoted.single.haskell"],
      "settings": {
        "foreground": "#ffd67a"
      }
    },
    {
      "name": "Haskell - Numbers",
      "scope": ["constant.numeric.haskell"],
      "settings": {
