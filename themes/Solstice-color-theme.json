{
  "name": "Solstice",
  "type": "dark",
  "colors": {
    "editor.background": "#161621",
    "editor.foreground": "#ffffff",
    "activityBar.background": "#0f0f18",
    "activityBar.foreground": "#ffffff",
    "activityBar.activeBorder": "#fa409d",
    "activityBar.activeBackground": "#1a1a28",
    "activityBar.activeFocusBorder": "#00ffff",
    "activityBar.inactiveForeground": "#999999",
    "activityBarBadge.background": "#007acc",
    "sideBar.background": "#13131c",
    "sideBar.foreground": "#ffffff",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBarSectionHeader.background": "#0f0f18",
    "statusBar.background": "#0f0f18",
    "statusBar.foreground": "#ffffff",
    "titleBar.activeBackground": "#0f0f18",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#0f0f18",
    "titleBar.inactiveForeground": "#999999",
    "tab.activeBackground": "#161621",
    "tab.inactiveBackground": "#0f0f18",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveForeground": "#999999",
    "editorGroupHeader.tabsBackground": "#0f0f18",
    "panel.background": "#13131c",
    "terminal.background": "#161621",
    "notifications.background": "#1a1a28",
    "notifications.foreground": "#ffffff",
    "notifications.border": "#fa409d",
    "notificationCenter.border": "#fa409d",
    "notificationCenterHeader.background": "#13131c",
    "notificationCenterHeader.foreground": "#ffffff",
    "notificationToast.border": "#fa409d",
    "notificationLink.foreground": "#00b3ff",
    "notificationsErrorIcon.foreground": "#FF5370",
    "notificationsWarningIcon.foreground": "#ffb86c",
    "notificationsInfoIcon.foreground": "#00b3ff",
    "button.background": "#3748e6",
    "button.foreground": "#ffffff"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#76718d"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#ffd67a"
      }
    },
    {
      "name": "String Quotes & Punctuation",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#ff9f50"
      }
    },
    {
      "name": "String Placeholders",
      "scope": [
        "string constant.other.placeholder",
        "constant.character.format.placeholder.other.python"
      ],
      "settings": {
        "foreground": "#a5c7cf"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#fb8c67"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#fa409d"
      }
    },
    {
      "name": "Control Keywords & Operators",
      "scope": [
        "keyword.control",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Parameters + References (unified color)",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#58a9fa"
      }
    },
    {
      "name": "Language Variables (this, self, super)",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#30f8ff"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#bf95ff"
      }
    },
    {
      "name": "Classes & Types",
      "scope": [
        "entity.name",
        "entity.name.type",
        "support.type",
        "support.class",
        "support.other.namespace"
      ],
      "settings": {
        "foreground": "#00b3ff"
      }
    },
    {
      "name": "Entity Types (Built-in)",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "meta.object.member",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Object Keys",
      "scope": [
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#fa409d"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#bb9cff"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#ffcc5f"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.sass",
        "support.type.property-name.less"
      ],
      "settings": {
        "foreground": "#bb9cff"
      }
    },
    {
      "name": "CSS Functions & Values",
      "scope": [
        "support.function.misc.css",
        "meta.function.variable.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#49effb"
      }
    },
    {
      "name": "CSS Units & Property Values",
      "scope": [
        "keyword.other.unit.px.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.css",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#6fb5ff"
      }
    },
    {
      "name": "JavaScript/TypeScript Specific",
      "scope": ["variable.other.object.js", "meta.block variable.other"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js",
        "meta.decorator",
        "meta.function.decorator.python",
        "entity.name.function.decorator.python"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Python - Function Definitions",
      "scope": [
        "meta.function.python entity.name.function.python",
        "support.function.magic.python"
      ],
      "settings": {
        "foreground": "#bf95ff"
      }
    },
    {
      "name": "Python - Function Calls",
      "scope": [
        "meta.function-call.python",
        "meta.function-call.generic.python"
      ],
      "settings": {
        "foreground": "#bf95ff"
      }
    },
    {
      "name": "Python - Arguments",
      "scope": ["meta.function-call.arguments.python"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Python - Magic Methods & Variables",
      "scope": [
        "support.function.magic.python",
